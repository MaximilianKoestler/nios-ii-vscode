{
  "name": "nios-ii",
  "displayName": "Nios II",
  "description": "Nios II Language Support for Nois II Assembly Language Programming",
  "publisher": "niosii",
  "version": "0.0.1",
  "repository": "https://github.com/estKey/nios-ii",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "niosii",
        "aliases": [
          "Nios II",
          "niosii"
        ],
        "extensions": [
          ".s",
          ".niosii",
          ".eds",
          ".elf"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "niosii",
        "scopeName": "source.s",
        "path": "./syntaxes/niosii.tmLanguage.json"
      }
    ]
  }
}
