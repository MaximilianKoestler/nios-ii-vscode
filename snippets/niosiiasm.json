{
  "separator": {
    "prefix": "separator",
    "description": "Snippet for a separator",
    "body": ["# -------------------------------------------------------\n$0"]
  },
  "heading": {
    "prefix": "heading",
    "description": "Snippet for a heading",
    "body": [
      "# -------------------------------------------------------",
      "# -- $1",
      "# -------------------------------------------------------\n$0"
    ]
  },
  "heading&description": {
    "prefix": "heading&description",
    "description": "Snippet for a heading with a block for description",
    "body": [
      "# -------------------------------------------------------",
      "# -- $1",
      "# -------------------------------------------------------",
      "# $2",
      "# -------------------------------------------------------\n$0"
    ]
  },
  "document": {
    "prefix": "document",
    "description": "Snippet for a document",
    "body": [
      "/* -------------------------------------------------------",
      "# $1 #",
      "$2",
      "------------------------------------------------------- */\n$0"
    ]
  },
  ".org": {
    "prefix": ".org",
    "description": "Snippet for a org",
    "body": [".equ\t\t$0\n"]
  },
  ".equ": {
    "prefix": ".equ",
    "description": "Snippet for a equ",
    "body": [".equ\t\t${1:var},\t\t${2:value}\n"]
  },
  ".section": {
    "prefix": ".section",
    "description": "Snippet for a section",
    "body": [".section\t\t$0\n"]
  },
  ".byte": {
    "prefix": ".byte",
