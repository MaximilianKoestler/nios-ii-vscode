{
  "separator": {
    "prefix": "separator",
    "description": "Snippet for a separator",
    "body": ["# -------------------------------------------------------\n$0"]
  },
  "heading": {
    "prefix": "heading",
    "description": "Snippet for a heading",
    "body": [
      "# -------------------------------------------------------",
      "# -- $1",
      "# -------------------------------------------------------\n$0"
    ]
  },
  "heading&description": {
    "prefix": "heading&description",
    "description": "Snippet for a heading with a block for description",
    "body": [
      "# -------------------------------------------------------",
      "# -- $1",
      "# -------------------------------------------------------",
      "# $2",
      "# -------------------------------------------------------\n$0"
    ]
  },
  "document": {
    "prefix": "document",
    "description": "Snippet for a document",
    "body": [
      "/* -------------------------------------------------------",
      "# $1 #",
      "$2",
      "------------------------------------------------------- */\n$0"
    ]
  },
  ".org": {
    "prefix": ".org",
    "description": "Snippet for a org",
    "body": [".equ\t\t$0\n"]
  },
  ".equ": {
    "prefix": ".equ",
    "description": "Snippet for a equ",
    "body": [".equ\t\t${1:var},\t\t${2:value}\n"]
  },
  ".section": {
    "prefix": ".section",
    "description": "Snippet for a section",
    "body": [".section\t\t$0\n"]
  },
  ".byte": {
    "prefix": ".byte",
    "description": "Snippet for a byte",
    "body": ["${1:var}:\t\t.byte\t\t${2:value}\n"]
  },
  ".word": {
    "prefix": ".word",
    "description": "Snippet for a word",
    "body": ["${1:var}:\t\t.word\t\t${2:value}\n"]
  },
  ".hword": {
    "prefix": ".hword",
    "description": "Snippet for a hword",
    "body": ["${1:var}:\t\t.hword\t\t${2:value}\n"]
  },
  ".skip": {
    "prefix": ".skip",
    "description": "Snippet for a skip",
    "body": ["${1:var}:\t\t.skip\n"]
  },
  ".char": {
    "prefix": ".char",
    "description": "Snippet for a char",
    "body": ["${1:var}:\t\t.asciz\t\t'${2:value}'\n"]
  },
  ".string": {
    "prefix": ".string",
    "description": "Snippet for a string",
    "body": ["${1:var}:\t\t.asciz\t\t\"${2:value}\"\n"]
  }
}
